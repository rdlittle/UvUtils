<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>

<Pane prefHeight="317.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.webfront.uvtool.controller.ProfileController">
    <children>
        <ImageView fx:id="imgSpinner" fitHeight="44.0" fitWidth="45.0" layoutX="438.0" layoutY="189.0" pickOnBounds="true" preserveRatio="true" />
        <Label fx:id="lblStatusMessage" layoutX="13.0" layoutY="282.0" prefHeight="16.0" prefWidth="391.0" />
          
        <Label layoutX="14.0" layoutY="81.0" text="Server name" />
        <Label layoutX="338.0" layoutY="81.0" text="Host" />
        <Label layoutX="13.0" layoutY="119.0" text="Account" />
        <Label layoutX="338.0" layoutY="117.0" text="Path" />
        <Label layoutX="14.0" layoutY="161.0" text="User name" />
        <TextField fx:id="txtUserName" layoutX="112.0" layoutY="156.0" />
        <Label layoutX="13.0" layoutY="203.0" text="Password" />
        <PasswordField fx:id="pwPassword" layoutX="112.0" layoutY="198.0" />
        <Button fx:id="btnTest" layoutX="292.0" layoutY="198.0" mnemonicParsing="false" onAction="#btnTestOnAction" text="Test connection" />
        <Button fx:id="btnSave" layoutX="438.0" layoutY="271.0" mnemonicParsing="false" onAction="#btnSaveOnAction" prefHeight="26.0" prefWidth="63.0" text="Save" />
        <Button fx:id="btnCancel" layoutX="515.0" layoutY="271.0" mnemonicParsing="false" text="Cancel" />
      <Label fx:id="lblProfile" layoutX="13.0" layoutY="23.0" text="Profile Title" />
      <ComboBox fx:id="cbProfiles" layoutX="112.0" layoutY="18.0" onAction="#cbProfilesOnAction" prefHeight="26.0" prefWidth="391.0" promptText="%msgSelectProfile" />
      <Button fx:id="btnAddProfile" layoutX="515.0" layoutY="18.0" mnemonicParsing="false" onAction="#btnAddProfileOnAction" text="+" />
      <ComboBox fx:id="cbServers" layoutX="112.0" layoutY="76.0" onAction="#cbServersOnAction" onKeyTyped="#cbServersOnKeyTyped" prefHeight="26.0" prefWidth="171.0" />
      <ComboBox fx:id="cbAccounts" layoutX="112.0" layoutY="114.0" onAction="#cbAccountsOnAction" prefHeight="26.0" prefWidth="171.0" />
      <Button fx:id="btnAddServer" layoutX="293.0" layoutY="76.0" mnemonicParsing="false" onAction="#btnAddServerOnAction" text="+" />
      <Button fx:id="btnAddAccount" layoutX="293.0" layoutY="114.0" mnemonicParsing="false" onAction="#btnAddAccountOnAction" text="+" />
      <TextField fx:id="txtHostName" layoutX="386.0" layoutY="76.0" />
      <TextField fx:id="txtPath" layoutX="386.0" layoutY="114.0" />
   
    </children>
</Pane>
